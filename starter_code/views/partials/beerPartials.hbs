{{#each beers}}
    <div class="container-beers">
        <div class="img-beer">
            <img src="{{image_url}}" alt="Beer image">
        </div>
        <h1 class="beer-name">{{name}}</h1>
        <p class="beer-tagline">{{tagline}}</p>
        <p class="beer-description">{{description}}</p>
    </div>
{{/each}}